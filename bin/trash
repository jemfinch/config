#!/bin/sh

TRASH="$HOME/Trash/`date +%Y%m%d`"
mkdir -p $TRASH
for file in "$@"; do
  if [ -e "$TRASH/`basename $file`" ]
  then
    TARGET="$TRASH/`basename $file`.`date +%s`"
  else
    TARGET="$TRASH"
  fi
  mv "$file" $TARGET
  # XXX Should actually gzip before mv
  gzip $TARGET >/dev/null 2>&1
done
